{
  "name": "template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "npm run build & npx cross-env NODE_ENV=prod DEBUG=app node -r ./tsconfig-paths-bootstrap.js ./dist/bin/www.js",
    "dev": "npx cross-env NODE_ENV=develop DEBUG=app npx nodemon --watch **/*.ts --exec ts-node -r tsconfig-paths/register ./bin/www.ts"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.2",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.24",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2"
  }
}
